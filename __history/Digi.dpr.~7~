program Digi;

uses
  MidasLib,
  Vcl.Forms,
  UMenu in 'UMenu.pas' {FMenu},
  UDataMod in 'UDataMod.pas' {Dm: TDataModule},
  UPadrao in 'UPadrao.pas' {FPadrao},
  UFuncoes in 'UFuncoes.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.Title := 'Digi';
